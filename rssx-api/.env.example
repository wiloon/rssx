# RSSX Configuration
RSS_SYNC_AUTO=true
SECURITY_KEY=your-security-key-here

# Redis Configuration
REDIS_ADDRESS=127.0.0.1:6379

# MySQL Configuration
MYSQL_ADDRESS=localhost:3306

# Sync Configuration (in minutes)
